{
    "experiment_name":"integration-pipeline",
    "datastore_name" : "workspaceblobstore",
    "cluster": {
        "cluster_cpu": {
            "cluster_name": "guillaume-cpu-low",
            "vm_size": "Standard_D4s_v3",
            "max_nodes": 1
        }
    },
    "environment": {
          "ml-cli_env": {
            "env_name": "integration-ml-cli",
            "dockerfile" : "./aml/Dockerfile",
            "conda_dependencies_file": "./aml/conda_dependencies.yml",
            "version": null
          },
         "light_env": {
            "env_name": "integration",
            "dockerfile" : null,
            "conda_dependencies_file": "./aml/conda_dependencies.yml",
            "version":  null
         }
    },
    "datasets": {
            "inputs": {
                "datasets": [{"name": "cats-dogs-others-integration",
                              "version": 36,
                              "directory": "data"}],
                "number_chunk": 1,
                "start_at_chunk_index": 0
            },
            "outputs": {
                "path_on_datastore": "integration/{date}",
                "datastore_name": "workspaceblobstore",
                "datasets": [{"name": "catdogother_mlcli_compare",
                            "pattern": "*",
                            "description": "Comparison file generated by ml-cli"}]
            }
        }
}